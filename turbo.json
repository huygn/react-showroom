{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        "dist-esm/**",
        "client-dist/**",
        "showroom/**",
        "showroom-spa/**",
        "build/**",
        "public/no-prerender/**",
        "public/some-subpath/**"
      ]
    },
    "test:e2e": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false
    }
  }
}
